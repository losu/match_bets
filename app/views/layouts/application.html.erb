<!DOCTYPE html>
<html>
<head>
  <title>MatchBets</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div>
<% if alert%>
<p>ALERT!: <%=alert%></p>
<% elsif notice %>
<p>notice: <%=notice%></p>
<% end %>

</div>
<%= yield %>


</body>
</html>
